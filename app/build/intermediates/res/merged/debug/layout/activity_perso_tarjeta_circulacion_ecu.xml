<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".PersoTarjetaCirculacionECActivity" >

    <TextView
        android:id="@+id/tarjetacirculacionecu_id_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_cedula" />
    <EditText 
        android:id="@+id/tarjetacirculacionecu_id_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_id_tv"
        android:inputType="text"
        android:text="@string/tc_ecu_id_default"/>
    
    <TextView
        android:id="@+id/tarjetacirculacionecu_placa_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_id_et"
        android:text="@string/label_placa" />
    <EditText 
        android:id="@+id/tarjetacirculacionecu_placa_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_placa_tv"
        android:inputType="text"
        android:text="@string/tc_ecu_placa_default"/>
    
    <TextView
        android:id="@+id/tarjetacirculacionecu_fechaemision_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_placa_et"
        android:text="@string/label_fecha_emision" />
    <EditText 
        android:id="@+id/tarjetacirculacionecu_fechaemision_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_fechaemision_tv"
        android:inputType="text"
        android:text="@string/tc_ecu_fem_default"/>
    
    <TextView
        android:id="@+id/tarjetacirculacionecu_fechaexpiracion_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_fechaemision_et"
        android:text="@string/label_fecha_vencimiento" />
    <EditText 
        android:id="@+id/tarjetacirculacionecu_fechaexpiracion_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_fechaexpiracion_tv"
        android:inputType="text"
        android:text="@string/tc_ecu_fex_default"/>
    
    <TextView
        android:id="@+id/tarjetacirculacionecu_fecharegistro_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_fechaexpiracion_et"
        android:text="@string/label_fecha_registro" />
    <EditText 
        android:id="@+id/tarjetacirculacionecu_fecharegistro_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/tarjetacirculacionecu_fecharegistro_tv"
        android:inputType="text"
        android:text="@string/tc_ecu_fre_default"/>
   
</RelativeLayout>
